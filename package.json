{
  "name": "wdio-v5-upgrade-helper-service",
  "version": "2.0.20",
  "description": "Utility library to allow renamed webdriverio v4 commands to be used with webdriverio 5.0. ",
  "main": "./build/index",
  "scripts": {
    "lint": "eslint --fix src/**/*.js e2e/**/*.js",
    "build": "npm run lint && rimraf build && babel src --out-dir build",
    "standalone": "mocha --timeout 99999 e2e/async/standalonetest.spec.js",
    "test": "wdio wdio.conf.js && npm run standalone"
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "author": "James R Davis III",
  "bugs": {
    "url": "https://github.com/jdavis61/wdio-v5-upgrade-helper-service/issues",
    "test": "wdio wdio.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdavis61/wdio-v5-upgrade-helper-service.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "@wdio/cli": "^5.21.0",
    "@wdio/local-runner": "^5.21.0",
    "@wdio/mocha-framework": "^5.18.7",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.18.7",
    "@wdio/sync": "^5.20.1",
    "cheerio": "^1.0.0-rc.3",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-wdio": "^5.13.2",
    "pre-commit": "latest",
    "webdriverio": "^5.21.0"
  }
}
